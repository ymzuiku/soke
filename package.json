{
  "name": "soke",
  "version": "2.0.4",
  "main": "umd/index.js",
  "types": "lib/index.ts",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "description": "轻量级快速校验数据",
  "private": false,
  "scripts": {
    "test": "jest --watchAll --detectOpenHandles",
    "test-c": "jest --coverage",
    "test-s": "jest --testResultsProcessor=jest-stare",
    "doc": "typedoc --out doc lib",
    "esm": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=esm --target=es6  --bundle  --external:react --external:react-ob2 --format=esm --minify --splitting",
    "cjs": "esbuild --define:process.env.NODE_ENV=\\\"production\\\" lib/index.ts --outdir=cjs --target=es6  --bundle  --external:react --external:react-ob --format=cjs --minify",
    "build": "yarn esm && yarn cjs"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^26.0.19",
    "error-get": "^1.0.1",
    "eslint": "^8.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-stare": "^2.2.0",
    "typedoc": "^0.19.2",
    "typescript": "3.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ymzuiku/soke.git"
  },
  "bugs": {
    "url": "git+https://github.com/ymzuiku/soke.git"
  },
  "homepage": "git+https://github.com/ymzuiku/soke.git"
}